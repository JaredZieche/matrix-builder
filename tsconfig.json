{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "lib": ["es6"],
        "outDir": "./lib",
        "rootDir": "./src",
        "declaration": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strictNullChecks": false
    },
    "exclude": ["node_modules", "lib", "**/*.test.ts"]
}
